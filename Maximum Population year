int maximumPopulation(int** logs, int logsSize, int* logsColSize) { 
    int pop[101] = {0};
    for (int i = 0; i < logsSize; i++) {
        pop[logs[i][0] - 1950]+=1;
        pop[logs[i][1] - 1950]-=1;    }
    int maxPop = 0, year = 1950, curr = 0;
    for (int i = 0; i < 101; i++) {
        curr += pop[i];
        if (curr > maxPop) {
            maxPop = curr;
            year = 1950 + i;
        }
    }

    return year;
}
